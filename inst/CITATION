citEntry(entry="Article",
  header="The underlying methodology papers can be cited as:",
  author =       "Hande Topa and \u00C1gnes J\u00F3n\u00E1s and Robert Kofler and Carolin Kosiol and Antti Honkela",
  title =        "Gaussian process test for high-throughput sequencing time series: application to experimental evolution",
  journal =      "Bioinformatics",
  year =         "2015",
  volume =       "31",
  pages =        "1762-1770",
  number =       "11",
  textVersion = paste(
    "Hande Topa, Agnes Jonas, Robert Kofler, Carolin Kosiol, Antti Honkela.",
    "Gaussian process test for high-throughput sequencing time series: application to experimental evolution.",
    "Bioinformatics 31(11):1762-1770, 2015."
  )
)

